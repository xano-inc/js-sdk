{
  "name": "@xano/js-sdk",
  "version": "3.0.0",
  "description": "Xano JavaScript SDK",
  "main": "lib/index",
  "types": "lib/index.d",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "build": "npx tsc && npx webpack --mode production",
    "watch:ts": "npx tsc --watch",
    "watch:web:dev": "npx webpack --watch --mode development",
    "watch:web:prod": "npx webpack --watch --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/xano-inc/js-sdk.git"
  },
  "keywords": [
    "xano",
    "nocode"
  ],
  "author": "xano",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xano-inc/js-sdk/issues"
  },
  "homepage": "https://xano.com",
  "dependencies": {
    "@server-sent-stream/web": "^1.0.3",
    "axios": "^1.12.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.1.2",
    "form-data": "^4.0.4",
    "jest": "^30.1.3",
    "jest-mock-axios": "^4.9.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/client.ts",
      "!src/index.ts",
      "!src/index.web.ts",
      "!src/**/enums/*.ts",
      "!src/**/interfaces/*ts",
      "!src/models/base-storage.ts"
    ]
  }
}
