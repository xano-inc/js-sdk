{
  "name": "@xano/js-sdk",
  "version": "1.0.0",
  "description": "Xano JavaScript SDK",
  "main": "lib/index",
  "types": "lib/index.d",
  "files": [
    "lib/index.js"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "build": "npx tsc",
    "watch": "npx tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/xano/js-sdk.git"
  },
  "keywords": [
    "xano",
    "nocode"
  ],
  "author": "xano",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/xano/js-sdk/issues"
  },
  "homepage": "https://gitlab.com/xano/js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/node-fetch": "^2.6.2",
    "babel-jest": "^29.0.3",
    "jest": "^29.0.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/index.ts",
      "!src/**/enums/*.ts",
      "!src/**/interfaces/*ts"
    ]
  }
}
